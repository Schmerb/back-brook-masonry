<% layout('layout') %>

<section id="who-we-are" class="who-we-are">
    <h2>Who We Are</h2>
    <div class="container">
        <ul class="employees">
            <% employees.forEach(user => { %>
                <li>
                    <%- partial('../blocks/employee', {user: user}) %>
                </li>
            <% }); %>
        </ul>
    </div>
</section>
    
    
