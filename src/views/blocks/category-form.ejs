<form class="category-form">
    <div class="select-wrap">
        <select>
            <% categories.forEach((category) => {%>
                <option value="<%= category.slug %>" 
                        <%= category.slug === categoryObj.slug ? 'selected':'' %>>
                    <%= category.type %>
                </option>
            <% }); %>
        </select>
        <svg class="icon icon-down-carrot"><use xlink:href="#icon-down-carrot"></use></svg>
    </div>
    <div class="input-wrap">
        <input class="search-bar" type="search" name="" value="" placeholder="Find A Project...">
        <svg class="icon icon-search-icon"><use xlink:href="#icon-search-icon"></use></svg>
        <svg class="icon icon-clear-icon hidden"><use xlink:href="#icon-clear-icon"></use></svg>
        <ul class="search-results"></ul>
    </div>
</form>

