<div class="project-slider-wrap">
    <h3 class="slider-title">More <%= projectObj.industry %> projects</h3>
    <!-- <div class="swipe-msg">Swipe to view</div> -->
    <svg class="icon icon-swipe-icon"><use xlink:href="#icon-swipe-icon"></use></svg>
    <div class="project-slider">
        <% projects.forEach(project => { %>
            <% if(project.name === projectObj.name) return %> <!-- Removes the current project from list  -->
            <div class="project-slide" style="background-image: url('<%= project.image %>');">
                <a class="slide-inner-wrap" href="<%= project.url %>#projects">
                    <div>
                        <h2><%= project.name %></h2>
                    </div>
                </a>
            </div>
        <% })  %>
    </div>
</div>